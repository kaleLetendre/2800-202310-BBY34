<div class="container bans collapse show" id="recommend-container">
  <!-- recommended bans -->
  <ul class="list-group list-group-horizontal bans-row">
    <% for (i = 0; i < 5; i++) { %>
    <li class="list-group-item">
      <img class="" src="<%= arr[i][0] !== 'blank' ? arr[i][1] : '/svg/empty.svg' %>" alt="">
      <div class="ban-overlay">
      </div>
      <img class="ban-img" src="/svg/ban-sign-icon.svg" alt="">
    </li>
    <% } %>
  </ul>
  <!-- recommended team -->
  <ul class="list-group list-group-horizontal bans-row">
    <% for (i = 5; i < 10; i++) { %>
    <li class="list-group-item">
      <img class="" src="<%= bans[i][0] !== 'blank' ? bans[i][1] : '/svg/empty.svg' %>" alt="">
    </li>
    <% } %>
  </ul>
</div>