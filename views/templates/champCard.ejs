<div class="champ-card d-flex justify-content-around col-6 p-0 <%= team %>" id="<%= team + '-card-' + position + 1 %>" onclick="handleChampClick()" data-id="<%= ((userTeam == team) ? 'champ' : 'enemy') + (position + 1) %>">
  <div class="info-container col-8">
    <h4><%= summonerName ? summonerName : '' %></h4>
    <!-- <p><%= role %></p> -->
  </div>
  <div class="image-container col-4 d-flex align-items-center justify-content-center" data-value="">
    <img class="rounded-circle img-fluid mx-auto d-block" src="<%= champ !== 'blank' ? imgURL : '/svg/plus.svg' %>" alt="<%= champ %> champion image">
  </div>
</div>