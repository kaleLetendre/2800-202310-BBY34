<%- include("templates/header") %>
<div class="container">
  <form action="/profileUpdate">
    <fieldset>
      <legend>User Profile</legend>
      <div class="mb-3">
        <label for="username" class="form-label">Username</label>
        <input type="text" id="username" class="form-control" placeholder="<%= username %>" disabled />
      </div>
      <div class="mb-3">
        <label for="email" class="form-label">Email</label>
        <input type="text" id="email" class="form-control" placeholder="<%= email %>" disabled />
      </div>
      <div class="mb-3">
        <label for="summonerName" class="form-label">Summoner Name</label>
        <input type="text" id="summonerName" class="form-control" placeholder="<%= summonerName %>" disabled />
      </div>
      <!-- <button class="btn btn-primary">Edit</button>
      <button type="submit" class="btn btn-primary">Save</button> -->
    </fieldset>
  </form>
</div>
<%- include("templates/footer") %>