<form method="POST" action="/askAI">
    <h3>User Team</h3>
    <% for (let i = 0; i < 11; i++) { %>
      <label for="row1_input<%= i %>">Input <%= i + 1 %></label>
      <input type="number" id="row1_input<%= i %>" name="row1_input<%= i %>">
    <% } %>
  
    <h3>Enemy Team</h3>
    <% for (let i = 0; i < 11; i++) { %>
      <label for="row2_input<%= i %>">Input <%= i + 1 %></label>
      <input type="number" id="row2_input<%= i %>" name="row2_input<%= i %>">
    <% } %>
  
    <button type="submit">Submit</button>
  </form>
  