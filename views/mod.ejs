<form action="/modify?target=<%-player%>" method="post" required>
    <div class="form-floating">
      <input
        name="champName"
        type="text"
        class="form-control"
        id="floatingInput"
        placeholder="Champion Name"
        required
      />
      <label for="floatingInput">Username</label>
      <button class="btn btn-primary btn-lg px-4 me-sm-3" type="submit">Enter</button>
    </div>
</form>
